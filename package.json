{
  "name": "dtsgenerator",
  "version": "3.12.1",
  "description": "TypeScript d.ts file generator for JSON Schema file",
  "bin": {
    "dtsgen": "bin/dtsgen"
  },
  "main": "dist/core/",
  "typings": "dist/core/index.d.ts",
  "scripts": {
    "build": "tsc -p ./tsconfig-build.json",
    "clean": "rimraf dist/ build/ {src,test}/**/*.{js,js.map} *.tsbuildinfo",
    "compile": "tsc -p .",
    "lint": "eslint --fix --ext ts src/ test/",
    "format": "prettier --write 'src/**/*.ts' 'test/*.ts' 'test/core/*.ts'",
    "precompile": "npm run lint && npm run format",
    "prepublishOnly": "npm run build",
    "pretest": "npm run compile",
    "do-test": "cross-env TS_NODE_FILES=true mocha --exit --require ts-node/register --require source-map-support/register --colors test/*_test.ts",
    "test": "nyc npm run do-test",
    "test:update-snapshot": "UPDATE_SNAPSHOT=1 npm run test",
    "coverage": "nyc report --reporter=lcov",
    "prepare": "husky install"
  },
  "directories": {
    "test": "test/"
  },
  "keywords": [
    "TypeScript",
    "JSON Schema",
    "OpenAPI"
  ],
  "author": "horiuchi",
  "license": "MIT",
  "homepage": "https://github.com/horiuchi/dtsgenerator",
  "repository": {
    "type": "git",
    "url": "git@github.com:horiuchi/dtsgenerator.git"
  },
  "bugs": {
    "url": "https://github.com/horiuchi/dtsgenerator/issues"
  },
  "engines": {
    "node": ">= 10.0"
  },
  "dependencies": {
    "commander": "^8.1.0",
    "cross-fetch": "^3.1.4",
    "debug": "^4.3.2",
    "glob": "^7.1.7",
    "https-proxy-agent": "^5.0.0",
    "js-yaml": "^4.1.0",
    "tslib": "^2.3.1",
    "typescript": "^4.3.5"
  },
  "devDependencies": {
    "@dtsgenerator/do-nothing": "^2.5.0",
    "@dtsgenerator/replace-namespace": "^1.5.0",
    "@dtsgenerator/single-quote": "^1.6.0",
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@types/debug": "^4.1.7",
    "@types/glob": "^7.1.4",
    "@types/js-yaml": "^4.0.2",
    "@types/mkdirp": "^1.0.2",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.6.1",
    "@types/prettier": "^2.3.2",
    "@typescript-eslint/eslint-plugin": "^4.29.1",
    "@typescript-eslint/parser": "^4.29.1",
    "coveralls": "^3.1.1",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.24.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-sort-imports-es6-autofix": "^0.6.0",
    "husky": "^7.0.1",
    "lint-staged": "^11.1.2",
    "mocha": "^9.0.3",
    "nyc": "^15.1.0",
    "prettier": "^2.3.2",
    "rimraf": "^3.0.2",
    "source-map-support": "^0.5.19",
    "ts-node": "^10.2.0"
  },
  "lint-staged": {
    "**/*.ts": [
      "prettier --write",
      "eslint --fix"
    ]
  }
}
